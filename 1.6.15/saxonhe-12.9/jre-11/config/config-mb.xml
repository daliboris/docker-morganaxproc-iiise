<?xml version="1.0" encoding="UTF-8"?>
<morgana-config xmlns="http://www.xml-project.com/morganaxproc">
    
    <!-- possible values: 
		see https://www.xml-project.com/manual/ch02.html#configuration_s1_1_s2_2
		
		saxon99
		Saxon10 (default)
		Saxon11 
		Saxon12 
		Saxon12-3
		jaxp
	-->
    <xslt-connector>saxon12-3</xslt-connector>
    
    <!-- possible values: 
		see https://www.xml-project.com/manual/ch02.html#configuration_s1_1_s2_3
		
		saxon99
		Saxon10 (default)
		Saxon11 
		Saxon12 
		Saxon12-3
	-->
    <xquery-connector>saxon12-3</xquery-connector>
    
    <!-- possible values: 
		see https://www.xml-project.com/manual/ch02.html#configuration_s1_1_s2_5
		schxslt (default)
		schxslt2 
		skeleton 
	-->
    <schematron-connector>schxslt</schematron-connector>
    
    <!-- possible values: 
		see https://www.xml-project.com/manual/ch02.html#configuration_s1_1_s2_7
		
		com.xml_project.morganaxproc3.ninemlConnector.NineMLConnector
		com.xml_project.morganaxproc3.markupblitzConnector.MarkupBlitzConnector
	--> 
    <ixml-connector>com.xml_project.morganaxproc3.markupblitzConnector.MarkupBlitzConnector</ixml-connector>
    
    <!-- Relative paths are resolved by uri of this file -->
    <path_to_SchXSLT_1>/opt/xproc/schxslt/schxslt-1.10.1/1.0</path_to_SchXSLT_1>
    <path_to_SchXSLT_2>/opt/xproc/schxslt/schxslt-1.10.1/2.0</path_to_SchXSLT_2>
    <path_to_iso_skeleton_schematron_1>/opt/xproc/schematron/xslt1</path_to_iso_skeleton_schematron_1>
    <path_to_iso_skeleton_schematron_2>/opt/xproc/schematron/xslt2</path_to_iso_skeleton_schematron_2>
    <path_to_SchXSLT2_transpiler>/opt/xproc/schxslt2-v1.4.4/transpile.xsl</path_to_SchXSLT2_transpiler>
</morgana-config>